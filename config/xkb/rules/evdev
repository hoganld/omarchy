! layout = keycodes
  usa = evdev+aliases(qwerty)
  code = evdev+aliases(qwerty)
  mac = evdev+aliases(qwerty)
  launch = evdev+aliases(qwerty)

! layout = types
  usa = complete
  code = complete
  mac = complete
  launch = complete

! layout = compat
  usa = complete
  code = complete
  mac = complete
  launch = complete

! layout = symbols
  usa = kog+us+inet(evdev)+kog(usa)
  code = kog+us+inet(evdev)+kog(code)
  mac = kog+us+inet(evdev)+kog(mac)
  launch = kog+us+inet(evdev)+kog(launch)

! include %S/evdev
