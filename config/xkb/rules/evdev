! layout = compat
  usa = complete
  code = complete
  mac = complete
  launch = complete
  launch_lite = complete

! layout = symbols
  usa = kog+us+inet(evdev)+kog(pc_mods)
  code = kog+us+inet(evdev)+kog(code)
  mac = kog+us+inet(evdev)+kog(mac)
  launch = kog+us+inet(evdev)+kog(launch)
  launch_lite = kog+us+inet(evdev)+kog(launch)+kog(escape_tilde)

